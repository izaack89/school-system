<div class="container-main">
  <h2 class="login100-form-title fs-70" style="color: white;">Welcome Professor, {{first_name}} {{last_name}}!</h2>

  {{#if professors_subjects.length}}
    <div class="row">
      {{#each professors_subjects as |subject|}}
        <div class="col-lg-4 col-md-6 mb-5">
          <div class="card text-dark bg-light mb-3">
            <div class="card-header" style="text-align: center;">
              <p class="fs-2 mt-2 text-dark"><i class="{{icon}}"></i> {{title}}</p>
            </div>
            <div class="card-body border-radius-lg position-relative overflow-hidden pb-4 px-sm-5 ">
              {{#each subject.subject_students as |student|}}
                    <p class="fs-6"><i class="fas fa-user-graduate"></i> Student: {{student.first_name}} {{student.last_name}} </p>
              {{/each}}
            </div>
              <div class="card-footer" style="text-align: center;">
                <a href="/subject/{{id}}/{{../professorId}}/professor"
                  class="font-weight-bold text-xs text-uppercase font-weight-bolder text-info icon-move-right">
                  Check Class Info
                  <i class="fas fa-arrow-right text-xs ms-1" aria-hidden="true"></i>
                </a>
              </div>
          </div>
        </div>


      {{/each}}

  {{/if}}

</div>
 <div id="calendar"></div>
