<div class='limiter'>
  <div class='container-login100'>
    <div class='wrap-login100'>

      <div class="login100-pic js-tilt" data-tilt>
        <img src="/images/img-01.png" alt="IMG">
      </div>

      <form class='form signup-form login100-form validate-form'>
        <span class="login100-form-title">
          Student Sign Up
        </span>

        <div class='wrap-input100 validate-input' data-validate='Valid name is required: no empty'>
          {{!-- <label for='name-signup' class='form-label'>Name:</label> --}}
          <input class='form-control input100' type='text' name="name" placeholder='Name' id='name-signup' />
          <span class="focus-input100"></span>
          <span class="symbol-input100">
            <i class="fas fa-user-edit" aria-hidden="true"></i>
          </span>
        </div>

        <div class='wrap-input100 validate-input' data-validate='Valid last name is required: no empty'>
          {{!-- <label for='lastname-signup' class='form-label'>Last name:</label> --}}
          <input class='form-control input100' type='text' name="last-name" placeholder='Last Name'
            id='lastname-signup' />
          <span class="focus-input100"></span>
          <span class="symbol-input100">
            <i class="fas fa-user-edit" aria-hidden="true"></i>
          </span>
        </div>

        <div class='wrap-input100 validate-input' data-validate='Valid email is required: ex@abc.xyz'>
          {{!-- <label for='email-signup' class='form-label'>Email:</label> --}}
          <input class='form-control input100' type='email' name="email" placeholder='Email' id='email-signup' />
          <span class="focus-input100"></span>
          <span class="symbol-input100">
            <i class="fa fa-envelope" aria-hidden="true"></i>
          </span>
        </div>

        <div class="wrap-input100 validate-input" data-validate="Password is required min 8 characters">
          {{!-- <label for='password-signup' class='form-label'>Password:</label> --}}
          <input class='form-control input100' type='password' name='pass' placeholder='Password'
            id='password-signup' />
          <span class="focus-input100"></span>
          <span class="symbol-input100">
            <i class='fa fa-lock' aria-hidden='true'></i>
          </span>
        </div>

        <div class='container-login100-form-btn'>
          <button class='btn login100-form-btn' type='submit'>Signup</button>
        </div>

        <div class='text-center p-t-136'>
          <a class='txt2' href='/studentlogin' id='professorlogin'>
            Login
            <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
          </a>
        </div>



      </form>
    </div>
    <script src="/vendor/jquery/jquery-3.2.1.min.js"></script>

    <script src="/vendor/bootstrap/js/popper.js"></script>
    <script src="/vendor/bootstrap/js/bootstrap.min.js"></script>

    <script src="/vendor/select2/select2.min.js"></script>

    <script src="/vendor/tilt/tilt.jquery.min.js"></script>

    <script src="/js/professorsignup.js"></script>
    <script src='/js/studentsignup.js'></script>
  </div>
</div>