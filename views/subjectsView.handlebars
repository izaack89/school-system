<div class="container-main">
    <h2 class="login100-form-title fs-70" style="color: white;">{{title}} </h2>
    <h3 class="login100-form-title fs-70" style="color: white;">Professor: {{professor.first_name}} {{professor.last_name}}</h3>    
    <a class="btn btn-primary" href="/{{routeback}}" role="button">Back To Main</a>
    <br>
    <br>
    {{#if subject_students.length}}

        <table id="myTable" class="table table-striped" style="width:100%;color:white!important">
            <thead>
                <tr>
                    <th>Student Name</th>
                    <th>Grade</th>
                </tr>
            </thead>
            <tbody>
                {{#each subject_students as |students|}}
                    {{#if (is_equal enrollment.professorId ../professorId) }}
                        <tr>
                            <td>{{first_name}} {{last_name}}</td>
                            <td>{{enrollment.grade}}</td>
                        </tr>
                    {{/if}}
                {{/each}}
            </tbody>
        </table>
    {{else}}
        <p style="color:white;text-align:center;font-size:20px">No Students for this subject</p>
    {{/if}}
</div>
<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script src="https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#myTable').DataTable();
    });

</script>